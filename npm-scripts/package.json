{
  "private": true,
  "scripts": {
    "build": "npm run build:sass --silent; npm run build:svg --silent; npm run build:scripts",
    "build:scripts": "node build/scripts.js; npm test;",
    "build:sass": "./build/sass.sh",
    "build:svg": "node --harmony-async-await build/svg.js",
    "watch": "npm run watch:sass --silent & npm run watch:scripts",
    "watch:scripts": "node build/scripts.js --watch",
    "watch:sass": "nodemon --quiet --watch resources/assets/sass/ --ext scss --exec \"npm run build:sass\"",
    "lint:js": "eslint --cache resources/assets/js/src/**; exit 0;",
    "lint:sass": "sass-lint -c .sass-lint.yml 'resources/assets/sass/**' -v -q; exit 0;",
    "test": "jasmine || true"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.18.0",
    "better-fetch": "^1.1.0",
    "cli-spinners": "^1.0.0",
    "concat": "^3.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-standard": "^2.0.1",
    "events": "^1.1.1",
    "fetch-intercept": "^2.2.1",
    "form-serialize": "^0.7.0",
    "fs": "0.0.2",
    "is-finite": "^1.0.1",
    "jasmine": "^2.5.3",
    "lodash": "^4.14.2",
    "node-sass": "^4.5.1",
    "node-sass-import": "^1.1.1",
    "nodemon": "^1.11.0",
    "ora": "^1.1.0",
    "path": "^0.12.7",
    "postcss-clean": "^1.0.2",
    "postcss-cli": "^3.0.0",
    "require-dir": "^0.1.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "shelljs": "^0.7.7",
    "svg-sprite": "^1.3.6",
    "uglify-js": "^2.8.15",
    "vue": "^2.1.4",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.4",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.2.0",
    "whatwg-fetch": "^2.0.1"
  }
}
